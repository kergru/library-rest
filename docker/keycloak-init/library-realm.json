      "credentials": [
        { "type": "password", "value": "pwd", "temporary": false }
      ],
      "realmRoles": ["LIBRARIAN"]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "LIBRARIAN",
        "description": "Bibliothekars-Rolle mit erweiterten Rechten"
      }
    ]
  },
  "clients": [
    {
      "clientId": "library-client-spa",
      "name": "Library Client SPA",
      "enabled": true,
      "publicClient": true,
      "rootUrl": "http://localhost:4200/",
      "baseUrl": "http://localhost:4200/",
      "redirectUris": [
        "http://localhost:4200/login/oauth2/code/keycloak",
        "http://localhost:4200/*"
      ],
      "defaultClientScopes": [
        "openid",
        "profile",
        "email"
      ],
      "optionalClientScopes": [
        "offline_access"
      ],
      "attributes": {
        "pkce.code.challenge.method": "S256"
      },
      "protocolMappers": [
        {
          "name": "realm roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "multivalued": "true",
            "userinfo.token.claim": "true",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "realm_access.roles",
            "jsonType.label": "String"
          }
        },
        {
          "name": "audience mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "config": {
            "included.client.audience": "library-client-spa",
            "id.token.claim": "true",
            "access.token.claim": "true"
          }
        }
      ]
    }
  ]
}
